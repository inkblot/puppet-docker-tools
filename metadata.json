{
  "name": "inkblot/docker_tools",
  "version": "1.0.1",
  "author": "inkblot",
  "license": "Apache-2.0",
  "summary": "Tools for working with docker",
  "source": "git://github.com/inkblot/puppet-docker-tools.git",
  "project_page": "https://github.com/inkblot/puppet-docker-tools",
  "issues_url": "https://github.com/inkblot/puppet-docker-tools/issues",
  "tags": [ "docker", "docker-machine", "docker-compose" ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "7.7" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "12.04", "14.04" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "6", "7" ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "6", "7" ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [ "20", "21", "22", "23" ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [ "9", "10", "10.1" ]
    },
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [ "5.5", "5.6", "5.7", "5.8" ]
    }
  ],
  "dependencies": [
    { "name": "puppet/archive" }
  ]
}
